{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "duplicate-browser-tab",
  "title": "Duplicate Browser Tab",
  "description": "Option + Shift + D - will duplicate the current tab of whatever browser you have active",
  "icon": "command-icon.png",
  "author": "alexcory",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "duplicate-browser-tab",
      "title": "Duplicate Browser Tab",
      "description": "Duplicate the current browser tab",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/**",
    "assets/**",
    "src/duplicate-tab.applescript"
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "duplicate-tab": "osascript ./assets/duplicate-tab.applescript"
  }
}
